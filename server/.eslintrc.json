{
  "extends": ["naver/es5", "prettier"],
  "env": {
    "browser": false,
    "es6": true,
    "node": true
  },
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "object-curly-spacing": 0,
    "class-methods-use-this": 0,
    "camelcase": 0
  },
  "ignorePatterns": ["*.test.js"]
}
